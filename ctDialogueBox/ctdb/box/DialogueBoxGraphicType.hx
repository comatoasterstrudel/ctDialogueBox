package ctDialogueBox.ctdb.box;

/**
 * an enum to dictate which type of graphic is being used for the dialogue box
 */
enum DialogueBoxGraphicType
{
    /**
     * you are using the default dialogue box
     */
    Default;
 
    /**
     * youre using a custom image
     */
    Custom;
    
    /**
     * youre using a custom image tied to an actor
     */
    CustomActor;    
}